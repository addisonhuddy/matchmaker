<br>
<h2>Students: <%= @students.count %></h2>

<table class="table">
  <th>Name</th>
  <th>Class</th>
  <th>Time Zone</th>
  <th>Week</th>
  <th>Weekend</th>
  <% @students.each do |student| %>
    <tr>
      <td><%= student.name %></td>
      <td><%= student.gatech_class %></td>
      <td><%= student.time_zone %></td>
      <td><%= student.week_preferred %></td>
      <td><%= student.weekend_preferred %></td>
    </tr>
  <% end %>
</table>
